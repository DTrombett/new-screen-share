{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"outDir": "dist",
		"lib": ["dom", "dom.iterable", "esnext"],
		"target": "ES2023",
		"isolatedModules": false,
		"noEmit": false
	},
	"include": ["app", ".next/types/**/*.ts", "*.ts"],
	"exclude": ["node_modules"]
}
